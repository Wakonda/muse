<div class="form-group row">
	{{ form_label(form.versionNumber, null, {'label_attr': {'class': 'col-sm-2 col-form-label'}}) }}
	<div class="col-sm-10">
		{{ form_errors(form.versionNumber) }}{{ form_widget(form.versionNumber, {'attr':{'class':'form-control'}}) }}
	</div>
</div>
<div class="form-group row">
	{{ form_label(form.releaseDate, null, {'label_attr': {'class': 'col-sm-2 col-form-label'}}) }}
	<div class="col-sm-10">
		{{ form_errors(form.releaseDate) }}{{ form_widget(form.releaseDate, {'attr':{'class':'form-control'}}) }}
	</div>
</div>
<div class="form-group row">
	{{ form_label(form.file, null, {'label_attr': {'class': 'col-sm-2 col-form-label'}}) }}
	<div class="col-sm-10">
		{{ form_errors(form.file) }}{% if entity.file is defined %}<p><span class="font-weight-bold underline">{{ 'admin.index.CurrentFile'|trans }} :</span> {{ entity.file }}</p>{% endif %}{{ form_widget(form.file, {'attr':{'class':'form-control'}}) }}
	</div>
</div>
<hr />
{{ form_widget(form.save, {'icon': 'fa-save'}) }}
{{ form_rest(form) }}