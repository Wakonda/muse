<ul>
	{% for entity in entities %}
		<li>
			<a href="{{ path('app_indexproverbius_read', {'id' : entity.id, 'slug' : entity.slug}) }}" >{{ entity.text|striptags|raw }}</a>
			<br>
			<i class="fas fa-chevron-circle-right"></i> 
			<span class="font-weight-bold">{{ "proverb.read.Proverb"|trans }} - {{ entity.country.title }}</span>
		</li>
	{% endfor %}
</ul>